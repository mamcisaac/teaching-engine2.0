{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": false,
    "isolatedModules": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true
  }
}
