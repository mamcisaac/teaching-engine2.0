${options.includeLearningGoals ?
  /* istanbul ignore next */
  (cov_25rcvv7gc().b[9][0]++, '6. Upcoming Learning Goals') :
  /* istanbul ignore next */
  (cov_25rcvv7gc().b[9][1]++, '')}
${options.includeUpcomingEvents ?
  /* istanbul ignore next */
  (cov_25rcvv7gc().b[10][0]++, '7. Important Dates & Events') :
  /* istanbul ignore next */
  (cov_25rcvv7gc().b[10][1]++, '')}
8. Home Support Tips
9. Closing Message

For each section, provide:
- title (English)
- titleFr (French)
- content (English, using simple HTML for formatting)
- contentFr (French, using simple HTML for formatting)

Format the response as a JSON array of sections.`);
  /* istanbul ignore next */
  cov_25rcvv7gc().s[11]++;
  try {
    const response =
    /* istanbul ignore next */
    (cov_25rcvv7gc().s[12]++, await openai.chat.completions.create({
      model: 'gpt-4o-mini',
      messages: [{
        role: 'system',
        content: systemPrompt
      }, {
        role: 'user',
        content: userPrompt
      }],
      temperature: tone === 'friendly' ?
      /* istanbul ignore next */
      (cov_25rcvv7gc().b[11][0]++, 0.8) :
      /* istanbul ignore next */
      (cov_25rcvv7gc().b[11][1]++, tone === 'formal' ?
      /* istanbul ignore next */
      (cov_25rcvv7gc().b[12][0]++, 0.5) :
      /* istanbul ignore next */
      (cov_25rcvv7gc().b[12][1]++, 0.6)),
      response_format: {
        type: 'json_object'
      }
    }));
    const content =
    /* istanbul ignore next */
    (cov_25rcvv7gc().s[13]++, response.choices[0]?.message?.content);
    /* istanbul ignore next */
    cov_25rcvv7gc().s[14]++;
    if (!content) {
      /* istanbul ignore next */
      cov_25rcvv7gc().b[13][0]++;
      cov_25rcvv7gc().s[15]++;
      throw new Error('No content generated');
    } else
    /* istanbul ignore next */
    {
      cov_25rcvv7gc().b[13][1]++;
    }
    const parsed =
    /* istanbul ignore next */
    (cov_25rcvv7gc().s[16]++, JSON.parse(content));
    const sections =
    /* istanbul ignore next */
    (cov_25rcvv7gc().s[17]++, (
    /* istanbul ignore next */
    (cov_25rcvv7gc().b[14][0]++, parsed.sections) ||
    /* istanbul ignore next */
    (cov_25rcvv7gc().b[14][1]++, [])).map((section, index) => {
      /* istanbul ignore next */
      cov_25rcvv7gc().f[4]++;
      cov_25rcvv7gc().s[18]++;
      return {
        id: `section-${Date.now()}-${index}`,
        title:
        /* istanbul ignore next */
        (cov_25rcvv7gc().b[15][0]++, section.title) ||
        /* istanbul ignore next */
        (cov_25rcvv7gc().b[15][1]++, 'Section'),
        titleFr:
        /* istanbul ignore next */
        (cov_25rcvv7gc().b[16][0]++, section.titleFr) ||
        /* istanbul ignore next */
        (cov_25rcvv7gc().b[16][1]++, 'Section'),
        content:
        /* istanbul ignore next */
        (cov_25rcvv7gc().b[17][0]++, section.content) ||
        /* istanbul ignore next */
        (cov_25rcvv7gc().b[17][1]++, ''),
        contentFr:
        /* istanbul ignore next */
        (cov_25rcvv7gc().b[18][0]++, section.contentFr) ||
        /* istanbul ignore next */
        (cov_25rcvv7gc().b[18][1]++, ''),
        isEditable: true,
        order: index
      };
    }));
    /* istanbul ignore next */
    cov_25rcvv7gc().s[19]++;
    return {
      sections
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_25rcvv7gc().s[20]++;
    console.error('Error generating newsletter content:', error);
    // Fallback to template-based generation
    /* istanbul ignore next */
    cov_25rcvv7gc().s[21]++;
    return generateFallbackNewsletter({
      dateRange,
      studentNames,
      tone,
      focusAreas,
      options
    });
  }
}
